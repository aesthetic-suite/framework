{
  "name": "aesthetic",
  "version": "0.0.0",
  "description": "Support a range of styling options for React components.",
  "keywords": [],
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "babel ./src -d ./lib",
    "watch": "babel --watch ./src -d ./lib",
    "lint": "eslint ./src ./tests",
    "flow": "flow check",
    "mocha": "yarn run mocha:in ./tests/*.test.js ./tests/**/*.test.js",
    "mocha:in": "mocha --opts ./mocha.opts",
    "test": "yarn run mocha",
    "posttest": "yarn run lint && yarn run flow",
    "preversion": "yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milesj/aesthetic.git"
  },
  "author": {
    "name": "Miles Johnson",
    "url": "http://milesj.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milesj/aesthetic/issues"
  },
  "homepage": "https://github.com/milesj/aesthetic#readme",
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "hoist-non-react-statics": "^1.2.0",
    "lodash.merge": "^4.6.0"
  },
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "aphrodite": "^1.1.0",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-flow-comments": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.25.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.36.0",
    "glamor": "^2.20.12",
    "jss": "^6.0.1",
    "mocha": "^3.1.2",
    "react": "^15.4.0",
