{
  "name": "@aesthetic/core",
  "version": "0.11.0",
  "description": "Aesthetic is a powerful type-safe, framework agnostic, CSS-in-JS library for styling components.",
  "keywords": [
    "aesthetic",
    "components",
    "style",
    "css",
    "css-in-js"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:aesthetic-suite/framework.git",
    "directory": "packages/core"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "esm/**/*.{js,map}",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "test.d.ts",
    "test.js"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aesthetic/system": "^0.9.9",
    "@aesthetic/types": "^0.6.7",
    "@aesthetic/utils": "^0.8.4"
  },
  "devDependencies": {
    "@aesthetic/addon-direction": "^0.4.7",
    "@aesthetic/addon-vendor": "^0.3.8",
    "@aesthetic/style": "^0.9.8"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "test": "src/test.ts"
    },
    "platform": "browser"
  }
}
