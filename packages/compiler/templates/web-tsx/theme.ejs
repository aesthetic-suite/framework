<% if (theme.extendedFrom) { -%>
import parentTheme from './<%= theme.extendedFrom -%>';

export default parentTheme.extend({
  palette: <%- include('../partials/web/palettes') -%>,
}, {
  contrast: '<%= theme.contrast -%>',
  scheme: '<%= theme.scheme -%>',
});

<% } else { -%>
import design from '..';

export default design.createTheme({
  contrast: '<%= theme.contrast -%>',
  scheme: '<%= theme.scheme -%>',
}, {
  palette: <%- include('../partials/web/palettes') -%>,
});
<% } -%>
