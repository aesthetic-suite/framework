<% if (theme.extendedFrom) { -%>
@import './<%= theme.extendedFrom -%>';
<% } else { -%>
@import '..';
<% } -%>

// contrast: <%= theme.contrast %>
// scheme: <%= theme.scheme %>
<% paletteTypes.forEach(type => { -%>

<% shadeRanges.forEach(shade => { -%>
$palette-<%= type -%>-color-<%= shade -%>: <%= data.palette[type].color[shade] -%> !default;
<% }) -%>
$palette-<%= type -%>-bg-base: <%= data.palette[type].bg.base -%> !default;
<% stateOrder.forEach(state => { -%>
$palette-<%= type -%>-bg-<%= state -%>: <%= data.palette[type].bg[state] -%> !default;
<% }) -%>
$palette-<%= type -%>-fg-base: <%= data.palette[type].fg.base -%> !default;
<% stateOrder.forEach(state => { -%>
$palette-<%= type -%>-fg-<%= state -%>: <%= data.palette[type].fg[state] -%> !default;
<% }) -%>
$palette-<%= type -%>-text: <%= data.palette[type].text -%> !default;
<% }) -%>
