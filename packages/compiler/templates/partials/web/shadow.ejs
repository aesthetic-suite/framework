{
  <% shadowSizes.forEach(size => { -%>
  <%= size -%>: {
    x: '<%= platform.unit(data.shadow[size].x) -%>', // <%= platform.px(data.shadow[size].x) %>
    y: '<%= platform.unit(data.shadow[size].y) -%>', // <%= platform.px(data.shadow[size].y) %>
    blur: '<%= platform.unit(data.shadow[size].blur) -%>', // <%= platform.px(data.shadow[size].blur) %>
    spread: '<%= platform.unit(data.shadow[size].spread) -%>', // <%= platform.px(data.shadow[size].spread) %>
  },
  <% }) -%>
}
