{
  "name": "@aesthetic/cli",
  "version": "0.8.4",
  "description": "CLI for the Aesthetic design and styling framework.",
  "keywords": [
    "aesthetic",
    "cli",
    "style",
    "framework",
    "suite"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:aesthetic-suite/framework.git",
    "directory": "packages/cli"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "types": "./dts/index.d.ts",
  "main": "./lib/index.js",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "templates/"
  ],
  "bin": {
    "aesthetic": "./lib/bin.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "dependencies": {
    "@aesthetic/compiler": "^0.8.4",
    "@aesthetic/core": "^0.10.4",
    "@boost/cli": "^2.10.4",
    "@boost/common": "^2.7.2",
    "fs-extra": "^10.0.0",
    "ink": "^3.0.8",
    "react": "^16.13.1"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "bin": "src/bin.ts"
    },
    "format": "lib",
    "platform": "node"
  }
}
